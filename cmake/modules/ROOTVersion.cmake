# Define the current ROOT version.
# Used to generate ROOT/config/RVersion.hxx and give a version number to packages.

set(ROOT_MAJOR_VERSION 6)

# Even for production branches, odd for dev branches.
set(ROOT_MINOR_VERSION 27)

# Even for releases; 99 for release branches before /00; odd otherwise
set(ROOT_PATCH_VERSION 01)

# The date of above release
set(ROOT_RELEASE_DATE "Jan 05 2022")
